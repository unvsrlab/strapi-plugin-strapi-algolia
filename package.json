{
  "name": "strapi-plugin-strapi-algolia",
  "version": "2.0.5",
  "description": "Index articles configured to Algolia",
  "keywords": [
    "strapi",
    "plugin",
    "algolia"
  ],
  "homepage": "https://market.strapi.io/plugins/strapi-plugin-strapi-algolia",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wizbii/strapi-algolia.git"
  },
  "license": "MIT",
  "author": {
    "name": "Wizbii"
  },
  "maintainers": [
    {
      "name": "Wizbii"
    }
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "eslint": "eslint --ignore-path .eslintignore .",
    "format": "prettier --write .",
    "lint": "npx concurrently --max-processes=50% \"npm:eslint\" \"npm:prettier\"",
    "prepare": "husky",
    "prettier": "prettier --check .",
    "release": "release-it --only-version",
    "test": "jest --runInBand --forceExit --detectOpenHandles",
    "test:ci": "npm run test --ci",
    "test:ts:back": "run -T tsc -p server/tsconfig.json",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "verify": "strapi-plugin verify",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.11",
    "@strapi/icons": "^2.0.0-rc.11",
    "@strapi/utils": "^5.12.3",
    "algoliasearch": "5.23.2",
    "deep-reduce": "^1.0.5",
    "react-intl": "^6.8.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@strapi/sdk-plugin": "^5.3.2",
    "@strapi/strapi": "^5.12.3",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "commitlint": "^19.8.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "release-it": "^17.10.0",
    "styled-components": "^6.1.17",
    "ts-jest": "^29.3.1",
    "typescript": "~5.7.3",
    "vite": ">=5.4.10"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "name": "strapi-algolia",
    "description": "Index articles configured to Algolia",
    "kind": "plugin",
    "icon": "plug"
  }
}
